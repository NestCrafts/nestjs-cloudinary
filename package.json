{
	"name": "nestjs-cloudinary",
	"version": "2.1.0",
	"description": "Cloudinary module for nestjs",
	"author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
	"license": "MIT",
	"readmeFilename": "README.md",
	"scripts": {
		"commit": "npx cz",
		"build": "rm -rf ./dist && tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint '{src,apps,libs,test}/**/*.ts'",
		"lint-fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
		"test:watch": "jest --watch",
		"test": "jest --config ./test/jest.json",
		"coverage": "jest --coverage",
		"coveralls": "yarn run coverage --coverageReporters=text-lcov | coveralls"
	},
	"keywords": [
		"nestjs",
		"config",
		"cloudinary"
	],
	"main": "dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/rubiin/nestjs-cloudinary"
	},
	"bugs": "https://github.com/rubiin/nestjs-cloudinary/issues",
	"dependencies": {
		"cloudinary": "^1.41.3",
		"sharp": "^0.33.5"
	},
	"peerDependencies": {
		"@nestjs/common": ">7.0.0"
	},
	"devDependencies": {
		"@nestjs/common": "^10.4.1",
		"@nestjs/core": "^10.4.1",
		"@nestjs/testing": "10.3.8",
		"@types/jest": "29.5.12",
		"@types/node": "20.12.11",
		"@typescript-eslint/eslint-plugin": "7.8.0",
		"@typescript-eslint/parser": "7.8.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.29.1",
		"husky": "^9.1.5",
		"jest": "29.7.0",
		"lint-staged": "^14.0.1",
		"prettier": "3.2.5",
		"reflect-metadata": "0.2.2",
		"rimraf": "^5.0.10",
		"rxjs": "^7.8.1",
		"ts-jest": "29.1.2",
		"typescript": "5.4.5"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,scss,yaml,yml}": [
			"prettier --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
