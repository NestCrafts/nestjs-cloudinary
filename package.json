{
	"name": "nestjs-cloudinary",
	"version": "1.0.12",
	"description": "Cloudinary module for nestjs",
	"author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
	"license": "MIT",
	"readmeFilename": "README.md",
	"scripts": {
		"commit": "npx cz",
		"build": "rm -rf ./dist && tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint '{src,apps,libs,test}/**/*.ts'",
		"lint-fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
		"test:watch": "jest --watch",
		"test": "jest --config ./test/jest.json",
		"coverage": "jest --coverage",
		"coveralls": "yarn run coverage --coverageReporters=text-lcov | coveralls"
	},
	"keywords": [
		"nestjs",
		"config",
		"cloudinary"
	],
	"files": [
		"dist",
		"*.d.ts"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/rubiin/nestjs-cloudinary"
	},
	"bugs": "https://github.com/rubiin/nestjs-cloudinary/issues",
	"dependencies": {
		"cloudinary": "^1.36.1",
		"sharp": "^0.31.3"
	},
	"peerDependencies": {
		"@nestjs/common": ">7.0.0"
	},
	"devDependencies": {
		"@nestjs/common": "^9.4.0",
		"@nestjs/core": "^9.4.0",
		"@nestjs/testing": "9.4.0",
		"@types/jest": "29.5.1",
		"@types/node": "18.16.0",
		"@typescript-eslint/eslint-plugin": "5.59.0",
		"@typescript-eslint/parser": "5.59.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5",
		"husky": "^8.0.3",
		"jest": "29.5.0",
		"lint-staged": "^13.2.1",
		"prettier": "2.8.8",
		"reflect-metadata": "0.1.13",
		"rimraf": "^5.0.0",
		"rxjs": "^7.8.0",
		"ts-jest": "29.1.0",
		"typescript": "5.0.4"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,scss,yaml,yml}": [
			"prettier --write"
		]
	},
	"main": "dist/index.js",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
